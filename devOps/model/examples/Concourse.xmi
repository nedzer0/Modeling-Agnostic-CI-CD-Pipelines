<?xml version="1.0" encoding="UTF-8"?>
<devOps:Pipeline
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:devOps="http://www.example.org/devOps"
    Name0="Concourse">
  <when Name4="booklit"
      Trigger="git"/>
  <job Name1="booklit"
      output="//@job.0/@input.0">
    <tool_framework_job
        Name5="linux"/>
    <command>
      <tool
          Name5="sh"/>
    </command>
    <command>
      <tool
          Name5="echo"/>
      <parameters
          Parameter="whoami"/>
    </command>
    <input Name3="some-input"/>
    <input Name3="some-input-2"/>
    <environment
        Key="SOME_PARAM"
        Value="some-default-value"/>
  </job>
  <job Name1="unit">
    <command>
      <tool
          Name5="run build"/>
    </command>
  </job>
  <pipeline_tool_framework
      Name5="booklit"/>
</devOps:Pipeline>
