<?xml version="1.0" encoding="UTF-8"?>
<devOps:Pipeline
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:devOps="http://www.example.org/devOps"
    Name0="Drone">
  <job Name1="job1">
    <tool_framework_job
        Name5="docker"/>
    <command>
      <tool
          Name5="go build"/>
    </command>
    <command>
      <tool
          Name5="go test"/>
    </command>
    <environment
        Key="GOOS"
        Value="linux"/>
    <environment
        Key="GOARCH"
        Value="amd64"/>
  </job>
  <job Name1="job2"
      Allow_faillure="true">
    <ifthenelse
        Condition="event">
      <if>
        <tool
            Name5="echo test"/>
      </if>
    </ifthenelse>
    <environment
        Key="GOOS"
        Value="linux"/>
  </job>
</devOps:Pipeline>
