<?xml version="1.0" encoding="ISO-8859-1"?>
<gHA_metamodel:Pipeline xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gHA_metamodel="http://www.example.org/gHA_metamodel" run_name="">
  <triggers xsi:type="gHA_metamodel:ScheduleTrigger" cron="0 * * * *"/>
  <triggers xsi:type="gHA_metamodel:WorkflowCallTrigger">
    <inputs name="username"/>
    <inputs type="boolean" name="perform_deploy"/>
    <outputs name="workflow_output1" value="${{ jobs.my_job.outputs.job_output1 }}" description=""/>
  </triggers>
  <jobs name="AgentCacheJob">
    <steps name="" shell="">
      <commands name="echo"/>
      <parameters parameter="hello-world"/>
      <actions xsi:type="gHA_metamodel:SaveCache" uses="actions/cache/save@v4" key="v1-{{ checksum &quot;yarn.lock&quot; }}">
        <paths>node_modules/workspace-a</paths>
        <paths>/home/ubuntu/.m2</paths>
      </actions>
    </steps>
    <envs key="MY-TEST-ENV" value="env_secret@78"/>
    <container image="my-docker-image" options="">
      <ports></ports>
      <volumes></volumes>
      <credential username="user24" password="secret"/>
      <envs key="env-34" value="hidden"/>
    </container>
  </jobs>
  <jobs name="conditional-job">
    <steps name="" shell="">
      <commands name="npm install java -v 1.4"/>
    </steps>
    <if condition="${{ ! startsWith(github.ref, 'refs/tags/') }}"/>
    <need>
      <jobs>AgentCacheJob</jobs>
    </need>
  </jobs>
  <jobs name="ArtifactMatrixJob">
    <steps name="" shell="">
      <commands name="echo This event is a pull request that had an assignee removed."/>
      <if condition="${{ github.event_name == 'pull_request' &amp;&amp; github.event.action == 'unassigned' }}"/>
    </steps>
    <strategy>
      <configs key="version">
        <values>0.1</values>
        <values>0.2</values>
        <values>0.3</values>
      </configs>
      <configs key="platform">
        <values>windows</values>
        <values>macos</values>
      </configs>
      <configs key="aba" type="EXCLUDE">
        <values>1</values>
        <values>5</values>
        <values>8</values>
      </configs>
      <configs key="color" type="INCLUDE">
        <values>green</values>
      </configs>
      <configs key="animal" type="INCLUDE">
        <values>cat</values>
      </configs>
      <configs key="os" type="EXCLUDE">
        <values>ubuntu</values>
      </configs>
    </strategy>
    <outputs name="workflow_output2" value="${{ jobs.my_job.outputs.job_output2 }}" description=""/>
    <actions xsi:type="gHA_metamodel:Artifact" uses="actions/upload-artifact@v4" name="code-coverage-report">
      <paths>jekyll/_site/docs/</paths>
    </actions>
  </jobs>
  <envs key="FOO" value="bar"/>
</gHA_metamodel:Pipeline>
